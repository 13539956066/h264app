<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    dynamicConfig="false"
    monitoring="off"
    updateCheck="false"
    xsi:noNamespaceSchemaLocation="ehcache.xsd" >
    <diskStore path="java.io.tmpdir/red5" />
    <defaultCache
        diskExpiryThreadIntervalSeconds="120"
        diskPersistent="false"
        eternal="false"
        maxElementsInMemory="4"
        memoryStoreEvictionPolicy="LFU"
        overflowToDisk="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120" />
    <cache
        name="org.red5.io.amf.Output.stringCache"
        eternal="false"
        maxElementsInMemory="1000"
        overflowToDisk="false"
        timeToIdleSeconds="1200" />
    <cache
        name="org.red5.io.amf.Output.serializeCache"
        eternal="false"
        maxElementsInMemory="200"
        overflowToDisk="false"
        timeToIdleSeconds="1200" />
    <cache
        name="org.red5.io.amf.Output.fieldCache"
        eternal="false"
        maxElementsInMemory="200"
        overflowToDisk="false"
        timeToIdleSeconds="1200" />
    <cache
        name="org.red5.io.amf.Output.getterCache"
        eternal="false"
        maxElementsInMemory="200"
        overflowToDisk="false"
        timeToIdleSeconds="1200" />
</ehcache>